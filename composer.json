{
  "name": "guysolamour/laravel-administrable-deploy",
  "description": "Deployment system built with ansible & ansistrano",
  "license": "MIT",
  "keywords": [
    "laravel",
    "administrable",
    "deploy"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Guy-roland ASSALE",
      "email": "rolandassale@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0.2",
    "illuminate/support": "^9.0"
  },
  "require-dev": {
    "orchestra/testbench": "^6.0",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Guysolamour\\Administrable\\Deploy\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Guysolamour\\Administrable\\Deploy\\Tests\\": "tests"
    }
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Guysolamour\\Administrable\\Deploy\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
